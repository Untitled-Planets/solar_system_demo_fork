script/source = "extends MeshInstance3D

#var clostes_list: Array[PickableObject] = []
#var current_idx: int = -1


func _process(_delta: float) -> void:
	
	if Engine.get_process_frames() % 2 == 0:
		var res_node: Array = get_tree().get_nodes_in_group(&\"pickable_object\")
		
		if res_node.size() > 0:
			var min_pos: Vector3 = res_node[0].global_position
			var min_dist: float = global_position.distance_squared_to(min_pos)
			
			if is_zero_approx(min_dist):
				look_at(min_pos)
			else:
				
				
				res_node.remove_at(0)
				
				for n in res_node:
					var aux: float = global_position.distance_squared_to(n.global_position)
					if aux < min_dist:
						min_dist = aux
						min_pos = n.global_position
				
				look_at(min_pos)

"

